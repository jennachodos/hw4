  <% if @current_user %>
  <div class="row">
    <div class="col-sm-9">
      <h1>Places</h1>
    </div>
    <div class="col-sm-3 text-end">
      <a href="/places/new/" class="btn btn-success">New Place</a>
    </div>
  </div>
<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
  <% else %>
  You must first <a href="/login">login</a>
  <% end %>
</ul>
