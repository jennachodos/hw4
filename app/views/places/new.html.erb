<h1>New Place</h1>
<% if @current_user %>
  <%= form_with :model => @place do |form| %>
    <div class="mb-3">
      <%= form.label "name" %>
      <%= form.text_field "name" %>
    </div>
    <div class="mb-3">
      <%= form.submit "Create Place", class: "btn btn-primary" %>
    </div>
    <%end %>
<% else %>
  You must first <a href="/login">login</a> to post.
<% end %>